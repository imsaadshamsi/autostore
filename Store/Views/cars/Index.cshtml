@model IEnumerable<Store.car>

@{
    ViewBag.Title = "Index";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Cars </h1>
    </div>
</div>



@foreach (var item in Model)
{
    
<div class="container services_main_section">
    <div class="row">
        <a href="~/cars">
            <div class="col-md-2 col-sm-2 col-xs-4 thumbnail">
                <img src="~/images/icons/req-waste.svg">
                <div class="services_section_modal_main ws_m caption">
                    <h5>@Html.DisplayFor(modelItem => item.name)</h5>
                    <p>
                        @Html.DisplayFor(modelItem => item.car_manufacturer.name)

                    </p>
                    <div class="services_section_inn">



                        <div class="services_section_modal_head">
                            <h3>Cars</h3>
                        </div>
                    </div>

                </div>
            </div>
        </a>

    </div>
    <div class="clearfix"></div>
</div>
}






